            "name": "standard_attrs",
            "value": false
          },
          {
            "name": "sequential_headings",
            "value": true
          }
        ]
      },
      {
        "name": "set_pdf_ua_standard",
        "params" : [
          {
            "name": "part_number",
            "value": 1
          }
        ]
      }
    ]
  },
